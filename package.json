{
  "name": "shario-main",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "buildJS": "browserify -t [ babelify --presets [ react ] ] public/js/src/index.js -o public/js/build/app.js",
    "watchJS": "watchify -t [ babelify --presets [ react ] ] public/js/src/index.js -o public/js/build/app.js -v",
    "buildSass": "node-sass public/sass/ -o public/css/",
    "watchSass": "node-sass -w public/sass/ -o public/css/"
  },
  "dependencies": {
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "jquery": "^3.1.0",
    "material-ui": "^0.15.4",
    "mongoose": "^4.5.10",
    "morgan": "~1.7.0",
    "node-sass": "^3.8.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "serve-favicon": "~2.3.0"
  }
}
